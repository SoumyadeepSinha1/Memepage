     <style>
      body{
        margin-top: 90px;
      }
      
      @media screen and (max-width:576px){
        .card-columns{
          column-count: 1;
          -webkit-column-count: 1;
          -moz-column-count: 1; 
        }
        .card{
          margin-left: 10%;
          margin-right: 10%;         
        }
        #name{
          display: none;
        }
      }
      @media screen and (min-width:576px){
        .card-columns{
          column-count: 2;
          -webkit-column-count: 2;
          -moz-column-count: 2;
        }
        #name{
          display: none;
        }
      }
      @media screen and (min-width:768px){
        .card-columns{
          column-count: 3;
          -webkit-column-count: 3;
          -moz-column-count: 3;
        }
        #smallname{
          display: none;
        }
        #name{
          display: inline;
        }
      }
      @media screen and (min-width:992px){
        .card-columns{
          column-count: 4;
          -webkit-column-count: 4;
          -moz-column-count: 4;
        }
        #smallname{
          display: none;
        }
        #name{
          display: inline;
        }
      }
      @media screen and (min-width:1500px){
        .card-columns{
          column-count: 5;
          -webkit-column-count: 5;
          -moz-column-count: 5;
        }
        #smallname{
          display: none;
        }
        #name{
          display: inline;
        }
      }      
      .card:hover #imagescard{
        filter: brightness(90%);
      }
      .card{
        border-radius: 10px 10px 10px 10px;
         border-width: 4px;
          padding: 1px;
          border-color: white;
      }
      .card:hover{
        border-color: rgba(239, 241, 244, 0.986);
        border-radius: 10px 10px 10px 10px;border-width: 4px;padding: 1px;
        background-color: rgba(239, 241, 244, 0.986);
        transition-duration: 200ms;  
        -moz-transition-duration: 200ms;
        -webkit-transition-duration: 200ms;
      }      
      .Search:focus{
        outline: none;
      }
      #toggler:focus{
        outline: none;
      }
      
